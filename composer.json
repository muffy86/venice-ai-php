{
    "name": "venice/venice-ai-php",
    "description": "Advanced PHP SDK for Venice AI API with comprehensive AI capabilities",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "ext-fileinfo": "*",
        "psr/log": "^3.0",
        "psr/http-client": "^1.0",
        "psr/http-message": "^2.0",
        "psr/cache": "^3.0",
        "psr/event-dispatcher": "^1.0",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/promises": "^2.0",
        "monolog/monolog": "^3.0",
        "predis/predis": "^2.2",
        "symfony/cache": "^6.3",
        "symfony/event-dispatcher": "^6.3",
        "symfony/serializer": "^6.3",
        "symfony/validator": "^6.3",
        "ramsey/uuid": "^4.7",
        "firebase/php-jwt": "^6.8",
        "league/oauth2-client": "^2.7",
        "nesbot/carbon": "^2.68",
        "vlucas/phpdotenv": "^5.5",
        "league/flysystem": "^3.15",
        "intervention/image": "^2.7",
        "spatie/async": "^1.5",
        "react/socket": "^1.14",
        "ratchet/pawl": "^0.4",
        "textalk/websocket-client": "^1.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.10",
        "husky/husky": "^8.0",
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Venice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Venice\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "phpcs": "phpcs --standard=PSR12 src/",
        "phpstan": "phpstan analyse src --level=max",
        "psalm": "psalm",
        "php-cs-fixer": "php-cs-fixer fix src --rules=@PSR12",
        "lint": "parallel-lint src tests",
        "check": [
            "@lint",
            "@phpcs",
            "@phpstan",
            "@psalm"
        ],
        "fix": [
            "@php-cs-fixer"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
