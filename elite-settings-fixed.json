        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
        "*.component.ts": "${capture}.component.html, ${capture}.component.scss, ${capture}.component.css"
