{
  // üöÄ ULTIMATE ELITE VS CODE SETTINGS - FINAL CLEAN VERSION
  // Ultra-modern development environment with AI, automation, and advanced tools

  // üé® ULTIMATE WORKBENCH & UI
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": "multiple",
  "workbench.statusBar.visible": true,
  "workbench.menuBar.visibility": "compact",
  "workbench.settings.enableNaturalLanguageSearch": true,
  "workbench.experimental.settingsProfiles.enabled": true,
  "workbench.editor.empty.hint": "hidden",

  // üé™ ULTIMATE EDITOR ENHANCEMENTS
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.fontSize": 14,
  "editor.fontFamily": "'Fira Code', 'JetBrains Mono', 'Cascadia Code', 'Source Code Pro', monospace",
  "editor.fontLigatures": true,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.rulers": [80, 100, 120],
  "editor.minimap.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.smoothScrolling": true,
  "editor.mouseWheelZoom": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.stickyScroll.enabled": true,

  // üöÄ AUTO-SAVE & AUTO-CONFIG
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,

  // üß† ULTIMATE AI INTEGRATIONS
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  "github.copilot.chat.followUps": "always",
  "github.copilot.chat.localeOverride": "en",
  "tabnine.experimentalAutoImports": true,
  "codeium.enableCodeLens": true,
  "blackboxAI.enabled": true,
  "continue.enableTabAutocomplete": true,

  // üî§ SPELL CHECKER SETTINGS - FIXED
  "cSpell.enabled": true,
  "cSpell.language": "en,en-US",
  "cSpell.enabledFileTypes": [
    "markdown",
    "text",
    "plaintext",
    "typescript",
    "javascript",
    "python",
    "php",
    "vue",
    "svelte",
    "html",
    "css",
    "scss",
    "json",
    "jsonc",
    "yaml",
    "yml"
  ],
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.showCommandsInEditorContextMenu": true,

  // üñ•Ô∏è TERMINAL SETTINGS
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "'Fira Code', 'JetBrains Mono', monospace",
  "terminal.integrated.env.windows": {},

  // üìö GIT SETTINGS
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.autoStash": true,

  // üåê LANGUAGE CONFIGURATIONS
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.tabSize": 4
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "on"
  },

  // üéõÔ∏è EXTENSION SETTINGS
  "prettier.singleQuote": true,
  "prettier.semi": true,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 100,
  "eslint.validate": ["javascript", "typescript", "vue"],
  "eslint.format.enable": true,
  "intelephense.format.enable": true,
  "intelephense.completion.triggerParameterHints": true,
  "python.formatting.provider": "black",
  "python.linting.enabled": true,

  // üîç SEARCH & EXPLORER
  "search.exclude": {
    "**/node_modules": true,
    "**/vendor": true,
    "**/dist": true,
    "**/build": true,
    "**/.git": true,
    "**/coverage": true
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,

  // üîß AI AGENT MCP SERVERS
  "mcp": {
    "servers": {
      "venice-ai-integration": {
        "command": "node",
        "args": ["c:/ai-agents/venice-integration.js"],
        "env": {
          "AI_PROVIDERS": "openai,anthropic,deepseek,openrouter",
          "AUTO_CONFIG": "true"
        }
      },
      "github-automation": {
        "command": "node",
        "args": ["c:/ai-agents/github-automation.js"],
        "env": {
          "GITHUB_TOKEN": "${workspaceFolder}/.env",
          "AUTO_WEBHOOKS": "true"
        }
      },
      "gitlab-cicd": {
        "command": "node",
        "args": ["c:/ai-agents/gitlab-cicd.js"],
        "env": {
          "GITLAB_TOKEN": "${workspaceFolder}/.env",
          "AUTO_DEPLOY": "true"
        }
      },
      "n8n-workflows": {
        "command": "node",
        "args": ["c:/ai-agents/n8n-automation.js"],
        "env": {
          "N8N_API_KEY": "${workspaceFolder}/.env",
          "AUTO_WORKFLOWS": "true"
        }
      }
    }
  },

  // üé® FILE ASSOCIATIONS
  "workbench.editorAssociations": {
    "*.md": "vscode.markdown.preview.editor",
    "*.ipynb": "jupyter-notebook"
  },

  // üõ°Ô∏è SECURITY & TOOLS
  "accessibility.signals.sounds.enabled": "auto",
  "accessibility.signalOptions.volume": 0.3,
  "amazonQ.suppressPrompts": {
    "amazonQChatDisclaimer": true
  },
  "snyk.advanced.cliPath": "C:\\Users\\aiech\\AppData\\Local\\snyk\\vscode-cli\\snyk-win.exe",
  "yaml.schemas": {
    "file:///c%3A/Users/aiech/.vscode/extensions/atlassian.atlascode-3.8.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "console-ninja.featureSet": "Community",
  "snyk.folderConfigs": [
    {
      "folderPath": "c:\\Users\\aiech\\OneDrive\\Documents\\GitHub\\venice-ai-php",
      "baseBranch": "master",
      "localBranches": ["master"]
    }
  ]
}
