Resources:
  AIAgent:
    Type: AWS::Wisdom::AIAgent
    Properties:
      Type: <String>
      Configuration: {}
      AssistantId: <String>
  AIAgentVersion:
    Type: AWS::Wisdom::AIAgentVersion
    Properties:
      AssistantId: <String>
      AIAgentId: <String>
  Api:
    Type: AWS::Serverless::Api
    Properties:
      Name: !Sub
        - ${ResourceName} From Stack ${AWS::StackName}
        - ResourceName: Api
      StageName: Prod
      DefinitionBody:
        openapi: '3.0'
        info: {}
        paths:
          /:
            get:
              responses: {}
      EndpointConfiguration: REGIONAL
      TracingEnabled: true
Metadata:
  AWS::Composer::Groups:
    Group:
      Label: Group
      Members:
        - AIAgentVersion
Transform: AWS::Serverless-2016-10-31